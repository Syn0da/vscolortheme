{
  "workbench.colorCustomizations": {
    "breadcrumb.background": "#1E1E1E",
    "panel.border": "#1E1E1E",
    "statusBar.noFolderBackground": "#1E1E1E",
    "list.inactiveSelectionBackground": "#1E1E1E",

    "tab.inactiveBackground": "#222222",
    "sideBar.background": "#222222",

    "sideBarSectionHeader.background": "#2f2f2f",
    "activityBar.background": "#2f2f2f",
    "titleBar.activeBackground": "#2f2f2f",
    "editorGroupHeader.tabsBackground": "#2f2f2f",

    "scrollbar.shadow": "#0000",
    "editorInlayHint.background": "#0000"
  },

  "editor.tokenColorCustomizations": {
    "[Visual Studio 2019 Dark]": {
      "textMateRules": [
        {
          "scope": "storage.type",
          "settings": {
            "foreground": "#569CD6",
            "fontStyle": "italic"
          }
        },
        {
          "scope": [
            "keyword.operator.arithmetic",
            "keyword.operator.logical.python",
            "punctuation.separator",
            "punctuation.definition.string.begin",
            "punctuation.definition.string.end",
            "punctuation.definition.comment",
            "punctuation.definition.decorator",
            "punctuation.section.function.begin"
          ],
          "settings": {
            "foreground": "#fe7e3e"
          }
        },
        {
          "scope": "keyword.operator.assignment",
          "settings": {
            "foreground": "#569CD6"
          }
        },
        {
          "scope": "punctuation.section.class.begin",
          "settings": {
            "foreground": "#FFDE73"
          }
        },
        {
          "scope": ["keyword.control", "keyword.operator.comparison.python"],
          "settings": {
            "foreground": "#ff447f"
          }
        }
      ],
      "strings": "#ffffff",
      "comments": "#5b5b5b",
      "numbers": "#ffffff"
    }
  },
  "editor.semanticTokenColorCustomizations": {
    "[Visual Studio 2019 Dark]": {
      "enabled": true
    },
    "rules": {
      "module": {
        "foreground": "#ffffff",
        "fontStyle": "bold"
      },
      "parameter": {
        "foreground": "#70ecfc",
        "fontStyle": "italic"
      },
      "variable": {
        "foreground": "#ffffff"
      },
      "class": {
        "foreground": "#80c75f",
        "fontStyle": "italic bold"
      },
      "clsParameter": {
        "foreground": "#fe7e3e",
        "fontStyle": "italic"
      },
      "magicFunction": {
        "foreground": "#fe7e3e"
      },
      "selfParameter": {
        "foreground": "#70ecfc",
        "fontStyle": "italic"
      },
      "method": {
        "foreground": "#fbdb71",
        "fontStyle": "italic"
      },
      "property": {
        "foreground": "#fbdb71"
      },
      "builtinConstant": {
        "fontStyle": "italic"
      },
      "function": {
        "foreground": "#70ecfc"
      },
      "*.decorator": "#80c75f",
      "decorator": "#fe7e3e",
      "*.readonly": "#569CD6"
    }
  }
}
