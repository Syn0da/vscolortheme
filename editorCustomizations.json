{
  "workbench.colorCustomizations": {
    "panel.border": "#1E1E1E",
    "statusBar.noFolderBackground": "#1E1E1E",
    "list.inactiveSelectionBackground": "#1E1E1E",

    "tab.inactiveBackground": "#222222",
    "sideBar.background": "#222222",

    "sideBarSectionHeader.background": "#2f2f2f",
    "activityBar.background": "#2f2f2f",
    "titleBar.activeBackground": "#2f2f2f",
    "editorGroupHeader.tabsBackground": "#222222",

    "editorInlayHint.background": "#0000"
  },
  "editor.tokenColorCustomizations": {
    "comments": "#4f606e",
    "types": "#b3ff5c",
    "functions": "#b3ff5c",
    "keywords": "#ff9d5c",
    "numbers": "#b3ff5c",
    "strings": "#FFffff",
    "variables": "#FFffff",
    "textMateRules": [
      {
        "scope": [
          "punctuation.definition",
          "keyword.operator",
          "punctuation.separator",
          "keyword.operator.logical.python"
        ],
        "settings": {
          "foreground": "#ffb85c"
        }
      },
      {
        "scope": [
          "punctuation.definition.string",
          "keyword.operator.assignment",
          "variable.other.constant"
        ],
        "settings": {
          "foreground": "#42ffc6"
        }
      }
    ]
  },
  "editor.semanticTokenColorCustomizations": {
    "enabled": true,
    "rules": {
      "module": { "foreground": "#FFffff", "fontStyle": "bold" },
      "class": {
        "fontStyle": "italic bold"
      },
      "*.readonly": "#42ffc6",
      "builtinConstant": { "foreground": "#42ffc6" },

      "property": "#ffdf28",
      "function": {
        "fontStyle": "italic"
      },
      "selfParameter": { "foreground": "#42ffc6", "fontStyle": "italic" },
      "parameter": { "fontStyle": "italic", "foreground": "#FFffff" },
      "clsParameter": {
        "foreground": "#ffdf28",
        "fontStyle": "italic"
      }
    }
  }
}
